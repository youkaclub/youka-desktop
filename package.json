  "keywords": [
    "karaoke"
  ],
  "config": {
    "forge": "./forge.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "Electron 8.0.0"
  ],
  "license": "GPL-3.0+",
  "husky": {
    "hooks": {
