{
  "name": "youka",
  "productName": "Youka",
  "description": "The Best Karaoke Maker",
  "version": "1.0.0",
  "author": "Youka",
  "main": ".webpack/main",
  "repository": "https://github.com/youkaclub/youka-desktop",
  "homepage": "https://app.youka.club",
  "scripts": {
    "start": "concurrently \"BROWSER=none craco start\" \"YOUKA_APP_URL=http://localhost:3000 electron src/main.js\"",
    "start:windows": "concurrently \".\\scripts\\react.cmd\" \".\\scripts\\electron.cmd\"",
    "dev": "concurrently \"BROWSER=none NODE_ENV=youka craco start\" \"YOUKA_APP_URL=http://localhost:3000 electron src/main.js\"",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` craco build",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "prettier --write src",
    "test": "mocha --recursive --timeout 10000 -r jsdom-global/register"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
